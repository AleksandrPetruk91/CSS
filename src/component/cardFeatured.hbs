{{#with data}}
  <div class='card'>
    <img
      class='card_image'
      src='{{photo}}'
      alt='Featured-img'
    />
    <h2 class='card_title'>
      {{heading}}
    </h2>
    <p class='card_text'>
      {{info}}
    </p>
    <a href='{{linkUrl}}' class='card_link'>
      {{linkText}}
    </a>
  </div>
{{/with}}